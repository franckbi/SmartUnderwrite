meta {
  name: Login Invalid Credentials
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email": "invalid@example.com",
    "password": "wrongpassword"
  }
}

assert {
  res.status: eq 401
}

tests {
  test("Invalid credentials return unauthorized", function() {
    expect(res.status).to.equal(401);
  });
}